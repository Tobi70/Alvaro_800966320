<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>index</title>
    <link rel="stylesheet" type="text/css" href="../css/main.css">
    <script src="../../js/milestone4.js"></script>
    <script src="jquery-3.5.1.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
</head>
<body>

<%- include ('partials/nav.ejs') -%>

<div id="div2">
    <button onclick="playshow()" id="play" style="margin-left: 51%">Play</button>
    <button onclick="stop()" id="stop">Stop</button>
</div>

<div class="grid-container">
    <% blogs.forEach((entry) => {
    %>
    <div id="blog" class="grid-item"><a id="title" href="/listPost/post/<%= entry._id %>" > <%= entry.title %> </a><p id="size">
        <%= entry.content %>
    </p></div>
    <% }) %>


</div>

<footer>
    <i>Copyright</i><img src="../img/Copyright-Symbol-PNG-File.png" class="blank"> <i>2020</i>
</footer>

</body>
</html>